{
  "kind": "collectionType",
  "collectionName": "generated_articles",
  "info": {
    "singularName": "generated-article",
    "pluralName": "generated-articles",
    "displayName": "Generated Article"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "string"
    },
    "notes": {
      "type": "richtext"
    },
    "chats": {
      "type": "component",
      "repeatable": true,
      "component": "open-ai-api.chat"
    },
    "source_template": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::source-template.source-template"
    },
    "election": {
      "type": "component",
      "repeatable": false,
      "component": "types.election"
    }
  }
}
